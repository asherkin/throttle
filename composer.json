{
    "name": "asherkin/throttle",
    "type": "project",
    "description": "Lightweight google-breakpad back-end written in PHP and backed by MySQL",
    "homepage": "https://crash.limetech.io/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Asher Baker",
            "email": "asherkin@limetech.io",
            "homepage": "https://limetech.io"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "ext-PDO": "*",
        "ext-ctype": "*",
        "ext-gmp": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo_mysql": "*",
        "ext-redis": "*",
        "ext-zlib": "*",
        "doctrine/doctrine-bundle": "^1.12",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "league/flysystem-bundle": "^1.5",
        "phacility/arcanist": "dev-stable",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.6",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/lock": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/stopwatch": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-symfony": "^0.11",
        "symfony/maker-bundle": "^1.15",
        "theofidry/psysh-bundle": "^3.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "phacility/arcanist",
                "version": "dev-stable",
                "source": {
                    "type": "git",
                    "url": "https://github.com/phacility/arcanist.git",
                    "reference": "origin/stable"
                }
            }
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "phpstan": "phpstan analyse src"
    }
}
